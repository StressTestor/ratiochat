{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/validation-key.txt",
            "dest": "/public/validation-key.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}